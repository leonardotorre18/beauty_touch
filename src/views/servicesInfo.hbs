<main class="main">
    <h1 class="title">{{data.title}}</h1>
    
    <section class="section">
    {{#each data.body}}
        {{#if title}}
        <h2 class="title">{{content}}</h2>
        
        {{else if param}}
        <p class="param">{{content}}</p>
        
        {{else if list}}
        <ul class="list">

            {{#each content}}
            <li>{{this}}</li>
            {{/each}}

        </ul>
        
        {{/if}}
    {{/each}}
    </section>
</main>